# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           com.hibu.api.petstore.client.controllers.Application.index()

GET /getPetById                     com.hibu.api.petstore.client.controllers.PetStoreClientController.getPetById()

GET /addPet                         com.hibu.api.petstore.client.controllers.PetStoreClientController.addPet()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
